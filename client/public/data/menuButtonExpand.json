{
  "version": 1,
  "description": "Expand menu button to a larger size when clicked",
  "viewBox": "0 0 200 300",
  "width": 200,
  "height": 300,
  "modifyPosition": {
    "x": 0,
    "y": 0
  },
  "animations": {
    "duration": 0.9,
    "loops": 1,
    "controlPointAnimations": {
      "sq-1": {
        "formula": {
          "x": {
            "expression": "55 - 80 * pow(sin((PI/2 * n)*(2-PI/2)), 0.5)+20"
          },
          "y": {
            "expression": "105 - 90 * pow(sin(PI/2 * n), 0.5)"
          }
        }
      },
      "sq-2": {
        "formula": {
          "x": {
            "expression": "125 + 60 * pow(sin(PI/2 * n), 0.5)"
          },
          "y": {
            "expression": "105 - 90 * pow(sin(PI/2 * n), 0.5)"
          }
        }
      },
      "sq-3": {
        "formula": {
          "x": {
            "expression": "125 + 60 * pow(sin(PI/2 * n), 0.5)"
          },
          "y": {
            "expression": "175 + 105 * pow(sin(PI/2 * n), 0.5)"
          }
        }
      },
      "sq-4": {
        "formula": {
          "x": {
            "expression": "55 - 40 * pow(sin(PI/2 * n), 0.5)"
          },
          "y": {
            "expression": "175 + 105 * pow(sin(PI/2 * n), 0.5)"
          }
        }
      }
    }
  },
  "fillPath": true,
  "closePath": true,
  "style": {
    "fill": "rgba(100, 180, 255, 0.1)",
    "stroke": "#FFFFFF",
    "strokeWidth": 0
  }
}
