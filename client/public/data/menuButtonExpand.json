{
  "version": 1,
  "description": "Expand menu button to a larger size when clicked",
  "viewBox": "0 0 200 300",
  "width": 200,
  "height": 300,
  "modifyPosition": {
    "x": 0,
    "y": 0
  },
  "variables":[
    {"smooth": "(0.5 *sin(PI*pow(n, 6)-(PI/2))+0.5)"}
  ],
  "animations": {
    "duration": 0.5,
    "loops": 1,
    "controlPointAnimations": {
      "sq-1": {
        "formula": {
          "x": {
            "expression": "70 - 55 * smooth"
          },
          "y": {
            "expression": "120 - 105 * (0.5 *sin(PI*pow(n, 3)-(PI/2))+0.5)"
          }
        }
      },
      "sq-2": {
        "formula": {
          "x": {
            "expression": "125 + 60 * 2*(0.5*sin((PI/2 * n)-(PI/2))+0.5)"
          },
          "y": {
            "expression": "120 - 105 * (0.5 *sin(PI*pow(n, 3)-(PI/2))+0.5)"
          }
        }
      },
      "sq-3": {
        "formula": {
          "x": {
            "expression": "125 + 60 * 2*(0.5*sin((PI/2 * n)-(PI/2))+0.5)"
          },
          "y": {
            "expression": "175 + 105 * 2*(0.5*sin((PI/2 * n)-(PI/2))+0.5)"
          }
        }
      },
      "sq-4": {
        "formula": {
          "x": {
            "expression": "70 - 55 * (0.5 *sin(PI*pow(n, 3)-(PI/2))+0.5)"
          },
          "y": {
            "expression": "175 + 105 * 2*(0.5*sin((PI/2 * n)-(PI/2))+0.5)"
          }
        }
      }
    },
    "styleAnimations": {
      "fill": "rgba(255, 255, 255, ${0.9 * (n)})"
    }
  },
  "fillPath": true,
  "closePath": true,
  "style": {
    "fill": "rgba(100, 180, 255, 0.1)",
    "stroke": "white",
    "strokeWidth": 0
  }
}
