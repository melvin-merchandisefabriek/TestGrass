{
  "id": "menu-button-square",
  "name": "Menu Button Square",
  "width": 120,
  "height": 40,
  "displayOptions": {
    "showControlPoints": false,
    "showAnchorPoints": false,
    "showPositionAnchor": false,
    "showBorder": true
  },
  "position": {
    "svg": { "x": 0, "y": 0 },
    "global": { "x": 500, "y": 50 }
  },
  "controlPoints": [
    { "id": "sq-1", "x": 10, "y": 10, "type": "anchor" },
    { "id": "sq-2", "x": 110, "y": 10, "type": "anchor" },
    { "id": "sq-3", "x": 110, "y": 30, "type": "anchor" },
    { "id": "sq-4", "x": 10, "y": 30, "type": "anchor" }
  ],
  "segments": [
    {
      "id": "sq-seg-1",
      "type": "line",
      "points": ["sq-1", "sq-2"],
      "style": { "stroke": "#4fc3f7", "strokeWidth": 2 }
    },
    {
      "id": "sq-seg-2",
      "type": "line",
      "points": ["sq-2", "sq-3"],
      "style": { "stroke": "#4fc3f7", "strokeWidth": 2 }
    },
    {
      "id": "sq-seg-3",
      "type": "line",
      "points": ["sq-3", "sq-4"],
      "style": { "stroke": "#4fc3f7", "strokeWidth": 2 }
    },
    {
      "id": "sq-seg-4",
      "type": "line",
      "points": ["sq-4", "sq-1"],
      "style": { "stroke": "#4fc3f7", "strokeWidth": 2 }
    }
  ],
  "fillPath": true,
  "closePath": true,
  "style": {
    "fill": "rgba(79, 195, 247, 0.3)",
    "fillOpacity": 0.5,
    "stroke": "#4fc3f7",
    "strokeWidth": 2
  }
}
